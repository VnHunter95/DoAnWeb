@model WebPhuKien.Models.NHASANXUAT

@{
    ViewBag.Title = "DeleteNSX";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Xóa thông tin Nhà sản xuất</h2>

<h3>Có chắc chắn muốn xóa vĩnh viễn nhà sản xuất này?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Tên nhà sản xuất:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Tennsx)
        </dd>

        <dt>
            Địa chỉ:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Diachi)
        </dd>

        <dt>
            Số điện thoại:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Sdtnsx)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Trở về trang trước", "NSX")
        </div>
    }
</div>
