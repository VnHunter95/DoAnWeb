@model WebPhuKien.Models.LIENHE

@{
    ViewBag.Title = "DelPhanhoi";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Xóa phản hồi</h2>

<h3>Có chắc chắn muốn xóa phản hồi này?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Họ tên người gửi:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Hoten)
        </dd>
        <dt>
            Email:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

        <dt>
            Số điện thoại:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Sdt)
        </dd>
        <dt>
            Tiêu đề bài gửi:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Tieude)
        </dd>

        <dt>
            Nội dung:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Noidung)
        </dd>

        

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" /> |
             @Html.ActionLink("Trở về", "Phanhoi")
        </div>
    }
</div>
